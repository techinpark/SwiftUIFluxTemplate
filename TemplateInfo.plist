<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>Easy make file for SwiftUIFlux Xcode Template</string>
	<key>Summary</key>
	<string>SwiftUIFlux Xcode Template</string>
	<key>SortOrder</key>
	<string>30</string>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>File</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
		<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>Name of the module to create</string>
			<key>Identifier</key>
			<string>swiftUIFluxModuleName</string>
			<key>Name</key>
			<string>Module name</string>
			<key>Default</key>
			<string>ModuleName</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>Input Module Name</string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_swiftUIFluxModuleName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
		</dict>
	</array>
</dict>
</plist>
